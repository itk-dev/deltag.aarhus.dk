    'hoeringsportal_hearing_type_address' => [
      'variables' => ['config' => NULL],
      'template' => 'hoeringsportal-hearing-type-address',
    ],
    'hoeringsportal_hearing_type_more_info' => [
      'variables' => ['config' => NULL],
      'template' => 'hoeringsportal-hearing-type-more-info',
    ],
    'hoeringsportal_hearing_warning' => [
      'variables' => ['config' => NULL],
      'template' => 'hoeringsportal-hearing-warning',
    ],
  ];
}
