{
    "description": "HÃ¸ringsportal",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mikkel Ricky <rimi@aarhus.dk>"
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "itk-dev": {
            "type": "composer",
            "url": "https://packagist.itkdev.dk"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org/"
        }
    },
    "require": {
        "php": "~7.2.0",
        "bower-asset/jquery-simple-color": "^1.2",
        "bower-asset/spectrum": "^1.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/color_field": "2.0-rc3",
        "drupal/console": "~1.0",
        "drupal/core": "~8.5.1",
        "drupal/ds": "^3.1",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity_browser": "^1.5",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/field_group": "3.x-dev",
        "drupal/flag": "^4.0@alpha",
        "drupal/image_widget_crop": "^2.2",
        "drupal/itk_cookie_message": "^1.0",
        "drupal/itk_media_entity": "^1.1",
        "drupal/itk_siteimprove": "^1.0",
        "drupal/media_entity_usage": "^1.0@alpha",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/samlauth": "^2.0",
        "drupal/toolbar_visibility": "^1.2",
        "drupal/viewsreference": "^1.2",
        "drush/drush": "^9.0.0",
        "itk-dev/composer-virtualenv": "^1.0",
        "onelogin/php-saml": "dev-3.0.0-namespaceless as 2.5",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "ln -sf ../../scripts/git/hooks/pre-commit .git/hooks/pre-commit"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "check-coding-standards/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "check-coding-standards": [
            "@check-coding-standards/phpcs"
        ],
        "apply-coding-standards/phpcs": [
            "vendor/bin/phpcbf --standard=phpcs.xml.dist"
        ],
        "apply-coding-standards": [
            "@apply-coding-standards/phpcs"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library", "name:jquery-simple-color","name:spectrum-colorpicker"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "enable-patching": true,
        "patches": {
            "drupal/paragraphs": {
                "Add paragraphs hook": "https://www.drupal.org/files/issues/2868155-16.patch"
            },
            "drupal/field_group": {
                "Possibility to add field group to advanced": "web/patches/field_group_node_sidebar.patch"
            }
        },
        "merge-plugin": {
            "require": [
                "web/modules/custom/hoeringsportal_deskpro/composer.json"
            ]
        }
    }
}
