{
    "description": "HÃ¸ringsportal",
    "type": "project",
    "name": "itk-dev/hoeringsportal",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mikkel Ricky <rimi@aarhus.dk>"
        }
    ],
    "repositories": {
        "toolbar-visibility": {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/toolbar_visibility-3127402"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": ["drupal/toolbar_visibility"]
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org/"
        },
        "drupal/hoeringsportal_data": {
            "type": "path",
            "url": "web/modules/custom/hoeringsportal_data",
            "options": {
                "symlink": false,
                "versions": {
                    "drupal/hoeringsportal_data": "1.0-dev"
                }
            }
        },
        "drupal/hoeringsportal_deskpro": {
            "type": "path",
            "url": "web/modules/custom/hoeringsportal_deskpro",
            "options": {
                "symlink": false,
                "versions": {
                    "drupal/hoeringsportal_deskpro": "1.0-dev"
                }
            }
        }
    },
    "require": {
        "php": "^8.1.0",
        "bower-asset/jquery-simple-color": "^1.2",
        "bower-asset/jqueryui-touch-punch": "dev-master",
        "bower-asset/spectrum": "^1.8",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.7",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/captcha": "^1.0@beta",
        "drupal/color_field": "^2.0",
        "drupal/config_ignore": "^2.2",
        "drupal/config_import_locale": "^2.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/ds": "^3.1",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity": "^1.2",
        "drupal/entity_browser": "^2.6",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/field_group": "^3.0",
        "drupal/flag": "^4.0@alpha",
        "drupal/hoeringsportal_data": "^1.0",
        "drupal/hoeringsportal_deskpro": "^1.0",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/openid_connect": "^2.0",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.3",
        "drupal/publication_date": "^2.0",
        "drupal/quick_node_clone": "^1.12",
        "drupal/redirect": "^1.6",
        "drupal/search_autocomplete": "^2.0",
        "drupal/system_status": "^2.8",
        "drupal/toolbar_visibility": "dev-3127402-drupal-9-readiness",
        "drupal/twig_tweak": "^2.1",
        "drupal/views_data_export": "^1.0",
        "drupal/viewsreference": "^1.2",
        "drupal/xls_serialization": "^1.2",
        "drush/drush": "^10.3",
        "itk-dev/composer-virtualenv": "^1.0",
        "itk-dev/itk_azure_video": "^1.1",
        "itk-dev/itk_media_entity": "^2",
        "itk-dev/itk_pretix": "^1.0",
        "itk-dev/itk_siteimprove": "^1.2",
        "itk-dev/web_accessibility_statement": "^1.0",
        "onelogin/php-saml": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^9.3",
        "drupal/upgrade_status": "^3.13",
        "phpstan/extension-installer": "^1.2",
        "mglaman/phpstan-drupal": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "code-analysis/phpstan": [
          "phpstan analyse --configuration=phpstan.neon"
        ],
        "code-analysis": [
          "@code-analysis/phpstan"
        ],
        "check-coding-standards/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "coding-standards-check": [
            "@check-coding-standards/phpcs"
        ],
        "apply-coding-standards/phpcs": [
            "scripts/phpcbf --standard=phpcs.xml.dist"
        ],
        "coding-standards-apply": [
            "@apply-coding-standards/phpcs"
        ],
        "post-install-cmd": [
            "@install-codestandards"
        ],
        "install-codestandards": [
            "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patches": {
          "deskpro/deskpro-api-client-php": {
            "Add typehinting to APIResponse to avoid drupal warnings in log": "patches/deskpro/deskpro-api-client-php/addTypeHinting.patch"
          }
        }
    }
}
